
class Parent{
	void show(){
		System.out.println("----");
	}
}

class Child extends Parent{
	void show(){
		System.out.println("+++++");
	}
}
public class Overriding {
	void show(Child c){
		System.out.println("+++++");
	}
	void show(Parent p){
		System.out.println("+++++");
	}

	public static void main(String[] args) {
		
		Parent parent = new Child();

	}

}
